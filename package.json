{
	"name": "react-roughjs",
	"version": "1.0.0-rc.1",
	"description": "React Bindings for Rough.js",
	"main": "dist/react-roughjs.js",
	"module": "dist/react-roughjs.es.js",
	"umd": "dist/react-roughjs.umd.js",
	"scripts": {
		"build": "npm-run-all rollup minify:* -s size",
		"contributors:add": "all-contributors add",
		"minify:cjs": "uglifyjs $npm_package_main -cm -o $npm_package_main -p relative  sourcemap-codec --in-source-map ${npm_package_main}.map --source-map ${npm_package_main}.map",
		"minify:umd": "uglifyjs $npm_package_umd -cm toplevel sourcemap-codec -o $npm_package_umd -p relative --in-source-map ${npm_package_umd}.map --source-map ${npm_package_umd}.map",
		"prepublish": "npm run build",
		"rollup": "rollup -c rollup.config.js",
		"size": "echo \"Gzipped Size: $(strip-json-comments --no-whitespace $npm_package_main | gzip-size)\"",
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/guimeisang/react-roughjs.git"
	},
	"author": "GM",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/guimeisang/react-roughjs/issues"
	},
	"homepage": "https://github.com/guimeisang/react-roughjs#readme",
	"eslintConfig": {
		"extends": "eslint-config-synacor"
	},
	"devDependencies": {
		"all-contributors-cli": "^4.10.1",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-plugin-external-helpers": "^6.22.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"enzyme": "^3.2.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.0",
		"eslint-config-synacor": "^3.0.0",
		"gzip-size-cli": "^2.1.0",
		"jest": "^22.4.3",
		"jsdom": "^16.2.2",
		"npm-run-all": "^4.1.2",
		"prop-types": "^15.6.0",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"rollup": "^0.53.0",
		"rollup-plugin-babel": "^3.0.3",
		"strip-json-comments": "^2.0.1",
		"uglifyjs": "^2.4.11"
	},
	"dependencies": {
		"roughjs": "^4.3.1"
	}
}
